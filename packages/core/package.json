{
  "name": "astro-link-preview",
  "version": "0.2.6",
  "description": "This Astro integration generates preview images for external links.",
  "private": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:debug": "tsup --config tsup.debug.ts",
    "build": "tsup --config tsup.config.ts",
    "install-playwright-deps": "playwright install chromium",
    "postinstall": "npx playwright install chromium"
  },
  "keywords": [
    "astro",
    "astro-integration",
    "astro-component",
    "preview",
    "link",
    "integration"
  ],
  "author": "nemurubaka",
  "license": "MIT",
  "homepage": "https://github.com/cijiugechu/astro-link-preview",
  "repository": {
    "type": "git",
    "url": "git@github.com:cijiugechu/astro-link-preview.git"
  },
  "bugs": "https://github.com/cijiugechu/astro-link-preview/issues",
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@types/node": "^18.11.18",
    "astro": "^2.4.4",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.8"
  },
  "dependencies": {
    "@napi-rs/image": "^1.6.1",
    "@playwright/test": ">=1.14.0",
    "html-rewriter-wasm": "^0.4.1",
    "kleur": "^4.1.4",
    "xxhash-wasm": "^1.0.2"
  },
  "peerDependencies": {
    "astro": ">=2.0.0"
  }
}
